class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
        int insertpos = 0; int i = 1; int cnt = 1;
        while(i < nums.size()){
            if(nums[i] == nums[insertpos]){
                i++;
            }
            else{
                cnt++;
                insertpos++;
                nums[insertpos] = nums[i];
                i++;
            }
        }
        return cnt;
    }
};
